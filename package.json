{
  "name": "blog-nuxt-sg",
  "version": "1.0.0",
  "description": "Personal website for Sean Groff built with Nuxt",
  "author": "Sean Groff",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "format": "eslint --fix --ext .js,.vue --ignore-path .gitignore . && prettier --write './**/*.{vue,js}'",
    "precommit-msg": "echo 'Performing Pre-commit checks...' && exit 0"
  },
  "pre-commit": [
    "precommit-msg",
    "lint",
    "format"
  ],
  "dependencies": {
    "@nuxtjs/axios": "^5.3.1",
    "nuxt": "^1.4.1",
    "nuxt-blog-module": "^0.0.1-8",
    "vuetify": "^1.0.19"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "cross-env": "^5.0.1",
    "eslint": "^4.15.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.13.4",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-loader": "13.7.2"
  }
}
